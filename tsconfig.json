{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "scripts/ping.js",
    "pages/index.tsx",
    "pages/api/index.js",
    "pages/sendsol/sendsol.jsx",
    "pages/_app.jsx"
  ],
  "maxNodeModuleJsDepth": 1,
  "skipLibCheck": true,
  "exclude": ["node_modules"]
}
